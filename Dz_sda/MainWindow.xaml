<Window x:Class="Dz_sda.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dz_sda"
        mc:Ignorable="d"
        Title="MainWindow" Name="Main" Height="650" Width="800">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/pngwing.com (1).png" HorizontalAlignment="Left" Width="60" Margin="10" ></Image>
        <TextBlock Text="Главная страница" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Grid Background="#bae3e8" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Back" Name="BtnBack" Width="55" Height="30" HorizontalAlignment="Right" Margin="5" Click="BtnBack_Click" Background="#bae3e8"  ></Button>
        <Button Content="Home" Name="BtnHome" Width="55" Height="30" HorizontalAlignment="Left" Margin="662,22,0,23"  Click="BtnHome_Click"  Background="#bae3e8"  ></Button>
    </Grid>
</Window>
